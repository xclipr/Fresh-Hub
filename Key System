MainFrame.Size = UDim2.new(0, 0, 0, 0)
MainFrame.BackgroundTransparency = 1
MainFrame.Position = UDim2.new(0.5, 0, 0.45, 0)

Tween(MainFrame, 0.6, {
    Size = UDim2.new(0, 360, 0, 260),
