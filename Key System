    -- Your actual script goes here
    print("script loaded - uerd was here")
end

if getgenv().RedExecutorKeySys then return end
getgenv().RedExecutorKeySys = true

-- Services
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")

-- ──────────────────────────────────────────────────────────────────────────────
--                               CONFIGURATION
-- ──────────────────────────────────────────────────────────────────────────────

local KeySystemData = {
    Name = "Fresh Hub",   -- ← Change your title here
    
    Colors = {
        Background       = Color3.fromRGB(245, 248, 255),   -- Very light blue-white
        FrameBorder      = Color3.fromRGB(0, 0, 0),         -- Strong black borders
        Title            = Color3.fromRGB(35, 130, 235),    -- Bright light blue
        InputField       = Color3.fromRGB(255, 255, 255),   -- Pure white
        InputFieldBorder = Color3.fromRGB(0, 0, 0),
        Button           = Color3.fromRGB(225, 240, 255),   -- Very pale blue
        ButtonHover      = Color3.fromRGB(200, 230, 255),   -- Hover state
        Error            = Color3.fromRGB(215, 50, 50),
        Success          = Color3.fromRGB(50, 180, 90),
        Discord          = Color3.fromRGB(88, 101, 242),
        Text             = Color3.fromRGB(20, 25, 40),      -- Dark text
        TextStroke       = Color3.fromRGB(0, 0, 0)          -- Black outline
    },
    
    Service       = "Fresh Hub",
    SilentMode    = false,
    DiscordInvite = "enterdiscordinvite",
    WebsiteURL    = "https://yourwebsite.com/",
    FileName      = "FreshHub/key.txt"
}

-- ──────────────────────────────────────────────────────────────────────────────
--                                 UTILITIES
-- ──────────────────────────────────────────────────────────────────────────────

local function Create(class, props)
    local obj = Instance.new(class)
    for k, v in pairs(props) do
        if k ~= "Parent" then
            obj[k] = v
        end
    end
    if props.Parent then obj.Parent = props.Parent end
    return obj
end

local function Tween(obj, t, props)
    local tween = TweenService:Create(obj, TweenInfo.new(t, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), props)
    tween:Play()
    return tween
end

-- ──────────────────────────────────────────────────────────────────────────────
--                                 INTERFACE
-- ──────────────────────────────────────────────────────────────────────────────

local ScreenGui = Create("ScreenGui", {
    Name = "Fresh Hub - Free",
    Parent = CoreGui,
    ResetOnSpawn = false,
    ZIndexBehavior = Enum.ZIndexBehavior.Sibling,
    DisplayOrder = 999
})

local MainFrame = Create("Frame", {
    Name = "MainFrame",
    Parent = ScreenGui,
    BackgroundColor3 = KeySystemData.Colors.Background,
    BorderColor3 = KeySystemData.Colors.FrameBorder,
    BorderSizePixel = 2,
    Position = UDim2.new(0.5, 0, 0.5, 0),
    AnchorPoint = Vector2.new(0.5, 0.5),
    Size = UDim2.new(0, 360, 0, 260),
    ClipsDescendants = true
})
Create("UICorner", {CornerRadius = UDim.new(0, 12), Parent = MainFrame})

-- Title Bar
local TitleBar = Create("Frame", {
    Name = "TitleBar",
    Parent = MainFrame,
    BackgroundColor3 = KeySystemData.Colors.Background,
    Size = UDim2.new(1, 0, 0, 38),
    BorderSizePixel = 0
})
Create("UICorner", {CornerRadius = UDim.new(0, 12, 0, 0), Parent = TitleBar})

Create("TextLabel", {
    Name = "Title",
    Parent = TitleBar,
    BackgroundTransparency = 1,
    Size = UDim2.new(1, -20, 1, 0),
    Position = UDim2.new(0.5, 0, 0.5, 0),
    AnchorPoint = Vector2.new(0.5, 0.5),
    Font = Enum.Font.GothamBold,
    Text = KeySystemData.Name .. " Key System",
    TextColor3 = KeySystemData.Colors.Title,
    TextSize = 17,
    TextXAlignment = Enum.TextXAlignment.Center,
    TextStrokeTransparency = 0.65,
    TextStrokeColor3 = KeySystemData.Colors.TextStroke
})

-- Input
local KeyInput = Create("TextBox", {
    Name = "KeyInput",
    Parent = MainFrame,
    BackgroundColor3 = KeySystemData.Colors.InputField,
    Text = "",
    PlaceholderText = "Enter your key here...",
    Position = UDim2.new(0.5, 0, 0.28, 0),
    Size = UDim2.new(0, 300, 0, 42),
    AnchorPoint = Vector2.new(0.5, 0),
    Font = Enum.Font.Gotham,
    TextSize = 15,
    TextColor3 = KeySystemData.Colors.Text,
    PlaceholderColor3 = Color3.fromRGB(150, 160, 180),
    TextXAlignment = Enum.TextXAlignment.Left,
    ClearTextOnFocus = false
})
Create("UICorner", {CornerRadius = UDim.new(0, 9), Parent = KeyInput})
Create("UIStroke", {
    Parent = KeyInput,
    Color = KeySystemData.Colors.InputFieldBorder,
    Thickness = 2,
    Transparency = 0
})
Create("UIPadding", {Parent = KeyInput, PaddingLeft = UDim.new(0, 14)})

-- Buttons
local SubmitButton = Create("TextButton", {
    Name = "ValidateButton",
    Parent = MainFrame,
    BackgroundColor3 = KeySystemData.Colors.Button,
    BorderColor3 = KeySystemData.Colors.FrameBorder,
    BorderSizePixel = 2,
    Position = UDim2.new(0.35, 0, 0.54, 0),
