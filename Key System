local DiscordButton = Create("TextButton", {
    Name = "DiscordButton",
    Parent = MainFrame,
    BackgroundColor3 = KeySystemData.Colors.Discord,
    BorderColor3 = KeySystemData.Colors.FrameBorder,
    BorderSizePixel = 2,
    Position = UDim2.new(0.5, 0, 0.76, 0),
    Size = UDim2.new(0, 260, 0, 42),
    AnchorPoint = Vector2.new(0.5, 0),
    Text = "Join Discord",
    Font = Enum.Font.GothamBold,
    TextSize = 15,
    TextColor3 = Color3.fromRGB(255,255,255),
    AutoButtonColor = false,
    TextStrokeTransparency = 0.7,
    TextStrokeColor3 = Color3.fromRGB(0,0,0)
})
Create("UICorner", {CornerRadius = UDim.new(0, 9), Parent = DiscordButton})

local StatusLabel = Create("TextLabel", {
    Name = "StatusLabel",
    Parent = MainFrame,
    BackgroundTransparency = 1,
    Position = UDim2.new(0.5, 0, 0.92, 0),
    Size = UDim2.new(0, 320, 0, 24),
    AnchorPoint = Vector2.new(0.5, 0),
    Font = Enum.Font.Gotham,
    Text = "",
    TextColor3 = KeySystemData.Colors.Error,
    TextSize = 13,
    TextXAlignment = Enum.TextXAlignment.Center,
    TextTransparency = 1,
    TextStrokeTransparency = 0.6,
    TextStrokeColor3 = KeySystemData.Colors.TextStroke
})

-- ──────────────────────────────────────────────────────────────────────────────
--                                 INTERACTIONS
-- ──────────────────────────────────────────────────────────────────────────────

local function ShowStatus(text, color)
    StatusLabel.Text = text
    StatusLabel.TextColor3 = color
    StatusLabel.TextTransparency = 0
    Tween(StatusLabel, 0.3, {TextTransparency = 0})
    
    task.spawn(function()
        task.wait(3.2)
        if StatusLabel.Text == text then
            Tween(StatusLabel, 0.6, {TextTransparency = 1})
        end
    end)
end

local function AddHover(button)
    button.MouseEnter:Connect(function()
        Tween(button, 0.2, {BackgroundColor3 = KeySystemData.Colors.ButtonHover})
    end)
    button.MouseLeave:Connect(function()
        Tween(button, 0.2, {BackgroundColor3 = KeySystemData.Colors.Button})
    end)
end

AddHover(SubmitButton)
AddHover(GetKeyButton)

-- Input focus animation
KeyInput.Focused:Connect(function()
    Tween(KeyInput.UIStroke, 0.25, {
        Color = KeySystemData.Colors.Title,
        Thickness = 2.8
    })
end)

KeyInput.FocusLost:Connect(function()
    Tween(KeyInput.UIStroke, 0.25, {
        Color = KeySystemData.Colors.InputFieldBorder,
        Thickness = 2
    })
end)

-- Get Key
local function openGetKey()
    local JunkieKeySystem = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
    
    local link = JunkieKeySystem.getLink(Config.api, Config.provider, Config.service)
    if link then
        if setclipboard then
            setclipboard(link)
            ShowStatus("Verification link copied to clipboard!", KeySystemData.Colors.Success)
        else
            ShowStatus("Link: " .. link, KeySystemData.Colors.Success)
        end
    else
        ShowStatus("Failed to generate link", KeySystemData.Colors.Error)
    end
end

-- Verify Key
local function validateKey()
    local key = KeyInput.Text:gsub("%s+", "")
    if key == "" then
        ShowStatus("Please enter a key", KeySystemData.Colors.Error)
        return
    end

    ShowStatus("Checking key...", Color3.fromRGB(255, 180, 60))
    
    local JunkieKeySystem = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
    
    local valid = JunkieKeySystem.verifyKey(Config.api, key, Config.service)
    
    if valid then
        ShowStatus("Key accepted! Loading...", KeySystemData.Colors.Success)
        
        Tween(MainFrame, 0.6, {
            Position = UDim2.new(0.5, 0, -0.6, 0),
            BackgroundTransparency = 1
        })
        
        task.delay(0.7, function()
            ScreenGui:Destroy()
            main()
        end)
    else
        ShowStatus("Invalid key! Try again", KeySystemData.Colors.Error)
    end
end

SubmitButton.MouseButton1Click:Connect(validateKey)
GetKeyButton.MouseButton1Click:Connect(openGetKey)

KeyInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then validateKey() end
end)

-- Dragging
local dragging, dragInput, dragStart, startPos

local function updateDrag(input)
    if dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end

TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

TitleBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(updateDrag)

-- Discord
DiscordButton.MouseButton1Click:Connect(function()
    local url = "https://discord.gg/" .. KeySystemData.DiscordInvite
    if setclipboard then
        setclipboard(url)
        ShowStatus("Discord invite copied!", Color3.fromRGB(100, 140, 255))
    else
        ShowStatus("Join: " .. url, Color3.fromRGB(100, 140, 255))
    end
end)

